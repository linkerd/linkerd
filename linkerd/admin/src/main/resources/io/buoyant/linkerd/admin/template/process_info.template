<div class="row text-center">
  <div id="process-info">
    <ul class="list-inline topline-stats">
      {{#each stats}}
        <li data-key="{{dataKey}}">
          <strong class="stat-label">{{description}}</strong>
          <span id="{{elemId}}" class="stat">{{value}}</span>
        </li>
      {{/each}}
    </ul>
  </div>
</div>
